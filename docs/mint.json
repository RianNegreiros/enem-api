{
    "$schema": "https://mintlify.com/schema.json",
    "name": "API ENEM",
    "openapi": "https://api.enem.dev/v1",
    "favicon": "/assets/favicon.svg",
    "logo": {
        "dark": "/assets/dark.svg",
        "light": "/assets/light.svg"
    },
    "colors": {
        "primary": "#0D9373",
        "light": "#07C983",
        "dark": "#0D9373",
        "anchors": {
            "from": "#0D9373",
            "to": "#07C983"
        }
    },
    "feedback": {
        "raiseIssue": true,
        "suggestEdit": true,
        "thumbsRating": true
    },
    "api": {
        "baseUrl": "https://api.enem.dev/v1"
    },
    "topAnchor": {
        "name": "Documentação"
    },
    "theme": "prism",
    "layout": "sidenav",
    "modeToggle": {
        "default": "dark"
    },
    "search": {
        "prompt": "Pesquisar..."
    },
    "topbarCtaButton": {
        "type": "github",
        "url": "https://github.com/yunger7/enem-api"
    },
    "tabs": [
        {
            "name": "API Reference",
            "url": "api-reference",
            "openapi": "https://api.enem.dev/v1"
        }
    ],
    "anchors": [
        {
            "name": "Home",
            "icon": "house",
            "url": "https://enem.dev"
        },
        {
            "name": "GitHub",
            "icon": "github",
            "url": "https://github.com/yunger7/enem-api"
        }
    ],
    "navigation": [
        {
            "group": "Começando",
            "pages": [
                "introduction",
                "self-hosting",
                "contributing"
            ]
        },
        {
            "group": "API v2",
            "pages": [
                "api-v2/get-all-exams",
                "api-v2/get-exam-by-id"
            ]
        },
        {
            "group": "API v1 Legada",
            "pages": [
                "api-v1/get-all-exams",
                "api-v1/get-exam-by-year"
            ]
        },
        {
            "group": "Referência da API",
            "pages": [
                "errors",
                "rate-limits"
            ]
        }
    ],
    "footerSocials": {
        "github": "https://github.com/yunger7/enem-api",
        "website": "https://enem.dev"
    }
}